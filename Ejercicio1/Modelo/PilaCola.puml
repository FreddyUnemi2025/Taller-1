@startuml

class Pila {
    - elementos: list
    + Pila(): void
    + push(elemento: Object): void
    + pop(): Object
    + isEmpty(): bool
    + top(): Object
    + size(): int
    + reverse(): Pila
    + pushAll(otraPila: Pila): Pila
}

class Cola {
    - elementos: list
    + Cola(): void
    + push(elemento: Object): void
    + pop(): Object
    + isEmpty(): bool
    + top(): Object
    + size(): int
    + reverse(): Cola
    + pushAll(otraCola: Cola): void
}

@enduml

